{
	"name": "Tuero Dark Theme",
	"colors": {
        "focusBorder": "#a7aab0",
        "foreground": "#d0d3d5",
		"editor.background": "#232326",
        // "window.inactiveBorder": "#ff0000",
        "activityBar.background": "#232326",
        "activityBar.foreground": "#d0d3d5",
        "activityBar.border": "#3f3f45",
        "dropdown.background": "#2f2f33",
        "dropdown.listBackground": "#2f2f33",
        "dropdown.border": "#3f3f45",
        "dropdown.foreground": "#d0d3d5",
        "sideBar.background": "#232326",
        "sideBar.foreground": "#d0d3d5",
        "sideBarTitle.foreground": "#d0d3d5",
        "sideBar.dropBackground": "#232326aa",
        "sideBarSectionHeader.background": "#232326",
        "sideBarSectionHeader.foreground": "#d0d3d5",
        "sideBarSectionHeader.border": "#3f3f45",
        "list.activeSelectionBackground": "#36363a",
        "list.activeSelectionForeground": "#d0d3d5",
        "list.dropBackground": "#36363a",
        "list.focusBackground": "#36363a",
        "list.focusForeground": "#d0d3d5",
        "list.focusOutline": "#3f3f45",
        "list.focusAndSelectionOutline": "#3f3f45",
        "list.hoverBackground": "#36363a",
        "list.hoverForeground": "#d0d3d5",
        "scrollbarSlider.activeBackground": "#d0d3d555",
        "scrollbarSlider.hoverBackground": "#d0d3d555",
        "scrollbarSlider.background": "#d0d3d533",
        // Inside the minimap
        "minimap.selectionHighlight": "#d0d3d588",
        "minimap.findMatchHighlight": "#d0d3d5",
        "minimapSlider.background": "#d0d3d555",
        "minimapSlider.hoverBackground": "#d0d3d555",
        "minimapSlider.activeBackground": "#d0d3d555",
        // Left gutter of minimap, redundant if we have right scroll bar gutter
        "minimapGutter.addedBackground": "#232326",
        "minimapGutter.deletedBackground": "#232326",
        "minimapGutter.modifiedBackground": "#232326",
        // Right and left gutter
        "editorGutter.addedBackground": "#008000cc",
        "editorGutter.deletedBackground": "#ff0000cc",
        "editorGutter.modifiedBackground": "#ffba00cc",
        // Title bar at the top
        "titleBar.activeBackground": "#232326",
        "titleBar.inactiveBackground": "#232326",
        "titleBar.activeForeground": "#d0d3d5",
        "titleBar.inactiveForeground": "#d0d3d555",
        "titleBar.border": "#3f3f45",
        // Menu bar for right-clicks
        "menu.background": "#2f2f33",
        "menu.foreground": "#d0d3d5",
        "menu.selectionBackground": "#d0d3d555",
        "menu.border": "#3f3f45",
        // Tabs and borders at the top
        "editorGroupHeader.tabsBorder": "#3f3f45",
        "tab.activeBackground": "#49494faa",
        "tab.unfocusedActiveBackground": "#49494faa",
        "tab.inactiveBackground": "#232326",
        "tab.unfocusedInactiveBackground": "#232326",
        "tab.border": "#3f3f45",
        "tab.hoverBackground": "#49494f",
        "tab.unfocusedHoverBackground": "#49494f",
        // Status bar at the bottom
        "statusBar.background": "#2f2f33",
        "statusBar.border": "#3f3f45",
        "statusBarItem.hoverBackground": "#49494f",
        "statusBarItem.remoteBackground": "#49494fbb",
        "statusBarItem.remoteForeground": "#d0d3d5",
        // Editor
        "editor.foreground": "#d0d3d5",
        "editorLineNumber.activeForeground": "#d0d3d5",
        "editorLineNumber.foreground": "#82828caa",
        "editor.wordHighlightBackground": "#82828588",
        "editor.wordHighlightStrongBackground": "#a0a0a888",
        "editor.findMatchBackground": "#a0a0a888",
        "editor.lineHighlightBackground": "#3f3f4588",
        "editorInlayHint.background": "#232326",
        "editorInlayHint.foreground": "#a8adb1aa"
        //"editor.lineHighlightBorder": "#3f3f4555",
		// "activityBarBadge.background": "#007acc",
		// "sideBarTitle.foreground": "#bbbbbb",
	},
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "templateType": {
            "foreground": "#ff0000",
        },
        "variable.globalScope": {
            "foreground": "#d0d3d5",
            "fontStyle": "bold italic"
        },
        "variable.readonly": {
            "foreground": "#d0d3d5",
            "fontStyle": "bold italic"
        },
        "class": {
            "foreground": "#e9ba63"
        },
        "type": {
            "foreground": "#e9ba63"
        },
        "method": {
            "foreground": "#57a5e5"
        },
        "function": {
            "foreground": "#57a5e5"
        },
        "parameter": {
            // "foreground": "#e09dad",
            "foreground": "#d0d3d5",
            "fontStyle": "italic bold"
        },
        "property": {
            "foreground": "#1ec4ae"
        },
        "namespace": {
            "foreground": "#e9ba63",
            "fontStyle": "italic bold"
        },
        "macro": {
            "foreground": "#1ec4ae",
            "fontStyle": "italic bold"
        },
    },
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6a737d"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
			],
			"settings": {
				"foreground": "#d0d3d5"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
                "keyword.control",
				"storage.type",
				"storage.modifier",
			],
			"settings": {
				"foreground": "#bb70d2"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
                "keyword.operator",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
                "keyword.other",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#bb70d2"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#57a5e5"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#d0d3d5"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
        {
			"name": "Function Argument",
			"scope": [
				"variable.parameter",
			],
			"settings": {
				"foreground": "#d0d3d5",
                "fontStyle": "italic  bold"
			}
		},
		{
			"name": "Number, Constant, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
			],
			"settings": {
				"foreground": "#cb9d73"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
                "string constant.other.placeholder",
                "punctuation.definition.string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#a4c38d",
                "fontStyle": "italic"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#e9ba63"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
